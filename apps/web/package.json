      "axios": "^1.9.0",
      "better-auth": "^1.2.8",
      "dotenv-flow-cli": "^1.1.1",
      "lucide-react": "^0.475.0",
      "next": "^15.2.3",
      "next-themes": "^0.4.4",
      "nuqs": "^2.4.3",
